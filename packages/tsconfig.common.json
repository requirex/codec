{
	"compileOnSave": true,
	"compilerOptions": {
		"composite": true,
		"declaration": true,
		"lib": [ "es5" ],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"removeComments": false,
		"sourceMap": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"target": "es5"
	}
}
